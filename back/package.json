    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "buffer-to-stream": "^1.0.0",
    "class-transform": "^0.7.0",

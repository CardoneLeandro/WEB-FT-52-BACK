  "scripts": {
    "go": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" && nest build && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./config/typeorm.config.ts && nest start --watch",
    "build": "node node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:full": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" && nest build && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npx typeorm",
    "typeorm:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./config/typeorm.config.ts",
    "migration:run": "npx typeorm migration:run -d ./dist/config/typeorm.config.js",
    "migration:generate": "npx typeorm migration:generate",
    "migration:runx": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./config/typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
